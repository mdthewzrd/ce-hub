{
  "name": "strategy-developer",
  "description": "Algorithmic trading strategy development, backtesting, and optimization agent",
  "version": "1.0.0",
  "author": "CE-Hub Trading Team",
  "capabilities": [
    "strategy creation and development",
    "backtesting with historical data",
    "parameter optimization",
    "performance analysis",
    "risk assessment",
    "walk-forward analysis",
    "monte carlo simulation",
    "strategy validation",
    "portfolio optimization",
    "benchmark comparison"
  ],
  "tools": [
    "vectorbt-engine",
    "quantconnect-backtesting",
    "talib-library",
    "risk-calculators",
    "portfolio-optimizer",
    "monte-carlo-simulator",
    "walk-forward-analyzer"
  ],
  "integrations": [
    "quantconnect-lean",
    "backtrader-framework",
    "zipline-engine",
    "polygon-historical-data",
    "yahoo-finance-api",
    "archon-knowledge-base"
  ],
  "dependencies": {
    "vectorbt-pro": "^0.25.0",
    "quantconnect": "^2.5.0",
    "backtrader": "^1.9.76.123",
    "zipline": "^1.3.0",
    "TA-Lib": "^0.4.25",
    "scipy": "^1.10.0",
    "scikit-learn": "^1.3.0",
    "plotly": "^5.15.0",
    "dash": "^2.11.0"
  },
  "strategy_types": [
    {
      "name": "trend_following",
      "indicators": ["SMA", "EMA", "MACD", "ADX"],
      "description": "Follow market trends using moving averages and momentum indicators"
    },
    {
      "name": "mean_reversion",
      "indicators": ["RSI", "Bollinger Bands", "Stochastic"],
      "description": "Exploit price reversals using oscillators and bands"
    },
    {
      "name": "breakout",
      "indicators": ["ATR", "ADX", "Volume"],
      "description": "Trade price breakouts from consolidation patterns"
    },
    {
      "name": "statistical_arbitrage",
      "indicators": ["Correlation", "Cointegration", "Spread"],
      "description": "Exploit statistical relationships between assets"
    }
  ],
  "backtesting_config": {
    "default_period": {
      "start_date": "2018-01-01",
      "end_date": "2023-12-31"
    },
    "initial_capital": 100000,
    "commission": 0.001,
    "slippage": 0.0001,
    "benchmark": "SPY",
    "risk_free_rate": 0.02
  },
  "optimization_parameters": {
    "max_iterations": 1000,
    "population_size": 100,
    "crossover_rate": 0.8,
    "mutation_rate": 0.1,
    "elitism_rate": 0.1,
    "optimization_methods": [
      "genetic_algorithm",
      "bayesian_optimization",
      "grid_search",
      "random_search"
    ]
  },
  "performance_metrics": [
    "total_return",
    "annual_return",
    "sharpe_ratio",
    "sortino_ratio",
    "max_drawdown",
    "win_rate",
    "profit_factor",
    "calmar_ratio",
    "information_ratio",
    "beta",
    "alpha"
  ],
  "risk_metrics": [
    "value_at_risk",
    "conditional_var",
    "expected_shortfall",
    "downside_deviation",
    "ulcer_index",
    "sterling_ratio"
  ],
  "api_endpoints": [
    {
      "method": "POST",
      "path": "/strategy/create",
      "description": "Create new trading strategy",
      "parameters": {
        "name": "string",
        "type": "string",
        "parameters": "object",
        "timeframe": "string",
        "universe": "string[]"
      }
    },
    {
      "method": "POST",
      "path": "/backtest/run",
      "description": "Run strategy backtest",
      "parameters": {
        "strategy_id": "string",
        "start_date": "string",
        "end_date": "string",
        "initial_capital": "number",
        "optimization": "boolean"
      }
    },
    {
      "method": "GET",
      "path": "/backtest/results/{backtest_id}",
      "description": "Get backtest results",
      "parameters": {
        "backtest_id": "string",
        "include_charts": "boolean"
      }
    },
    {
      "method": "POST",
      "path": "/strategy/optimize",
      "description": "Optimize strategy parameters",
      "parameters": {
        "strategy_id": "string",
        "parameter_ranges": "object",
        "optimization_method": "string",
        "objective_function": "string"
      }
    }
  ],
  "monitoring": {
    "metrics": [
      "backtest_execution_time",
      "optimization_convergence_rate",
      "strategy_count",
      "backtest_success_rate",
      "parameter_space_explored"
    ],
    "alerts": {
      "backtest_failure_rate": 0.1,
      "optimization_timeout": 3600,
      "memory_usage_threshold": 8192
    }
  }
}