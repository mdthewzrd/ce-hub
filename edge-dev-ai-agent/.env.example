# Configuration

# OpenRouter API Key (get from https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Model to use (via OpenRouter)
# Options: anthropic/claude-sonnet, anthropic/claude-opus-4, openai/gpt-4-turbo, etc.
DEFAULT_MODEL=anthropic/claude-sonnet

# Fallback model (if primary unavailable)
FALLBACK_MODEL=anthropic/claude-3-haiku

# Archon MCP Configuration
ARCHON_MCP_URL=http://localhost:8051
ARCHON_MCP_TIMEOUT=30

# EdgeDev Platform
EDGEDEV_BACKEND_URL=http://localhost:5666
EDGEDEV_DASHBOARD_URL=http://localhost:5665

# Polygon API (for market data)
POLYGON_API_KEY=your_polygon_api_key_here

# Application
APP_HOST=0.0.0.0
APP_PORT=7447
DEBUG=false
